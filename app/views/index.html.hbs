<!DOCTYPE html>
<html lang=en>
<head>

<meta charset=utf-8>

<title>Ellenoir</title>

<!-- Dependencies from Various CDNs
     ============================== -->

    <!-- Modernizr 2.6.2 --><script src=//cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js></script>
    <!-- Twitter Bootstrap 2.2.2 --><link rel=stylesheet href=//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/css/bootstrap-combined.min.css>

<!-- Modules files
     ============= -->

{{#each styles}}
    <link rel=stylesheet/less type=text/css href="{{url}}">
{{/each}}

    <!-- Less.js 1.3.3 --><script src=//cdnjs.cloudflare.com/ajax/libs/less.js/1.3.3/less.min.js></script>

</head>
<body>

<section id=app>
  <div class="navbar navbar-inverse navbar-static-top">
    <div class=navbar-inner>
      <div class=container>
        <span class=brand>Ellenoir</span>
      </div>
    </div>
  </div>

  {{#each widgets}}
    <article id="widget-{{module}}" class=ellenoir-widget data-module="{{module}}">

      {{{ contents }}}

    </article>
  {{/each}}

</section>

<!-- Dependencies from various CDNs
     ============================== -->

    <!-- jQuery 1.8.3 --><script src=//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.js></script>
    <!-- Twitter Bootstrap 2.2.2 --><script src=//netdna.bootstrapcdn.com/twitter-bootstrap/2.2.2/js/bootstrap.js></script>
    <!-- Lodash 1.0.0-rc.3 --><script src=//cdnjs.cloudflare.com/ajax/libs/lodash.js/1.0.0-rc.3/lodash.min.js></script>

<!-- Modules files
     ============= -->

{{#each scripts}}
    <script src="{{url}}"></script>
{{/each}}

</body>
</html>
